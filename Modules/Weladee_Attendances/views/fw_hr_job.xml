<odoo>
    <data>
        <record id="weladee_fw_hr_job_form" model="ir.ui.view">
            <field name="name">weladee_fw_hr_job_form</field>
            <field name="model">hr.job</field>
            <field name="inherit_id" ref="hr.view_hr_job_form"/>
            <field name="arch" type="xml">                
               <xpath expr="//notebook[last()]" position="inside">
                   <page string="Job ads">
                    
                   </page>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
